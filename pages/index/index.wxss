.login-container{
  height: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #f8f8f8;
  position: relative;
  overflow: hidden;
}

.headimage{
  border-radius: 50%;
  height: 180rpx;
  width: 180rpx;
  margin-top: 200rpx;
  margin-bottom: 100rpx;
}

.btn-login{
  width: 90%;
  border-radius: 100px;
  margin: 15rpx 0;
  background-color:rgb(7, 193, 96);
  
}

.tips-text{
  font-size: 12px;
  color: gray;
}